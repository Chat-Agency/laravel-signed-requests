{
    "name": "chatagency/laravel-signed-requests",
    "description": "A package to (hopefully) make custom signed requests easier.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jorge Morales",
            "email": "jorge@chatagency.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0|^8.1",
        "laravel/framework": "^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "ChatAgency\\LaravelSignedRequests\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ChatAgency\\LaravelSignedRequests\\LaravelSignedRequestsServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
